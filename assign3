import numpy as np
n=int(input("enter size of 1st array"))
a=np.zeros(n,dtype=int)
for i in range(len(a)):
    a[i]=int(input("enter element"))
n1=int(input("enter size of 2st array"))
b=np.zeros(n,dtype=int)
for i in range(len(a)):
    b[i]=int(input("enter element"))
    
arry1=a.reshape(3,3)
arry2=b.reshape(3,3)
c=np.add(arry1,arry2)
print(c)
