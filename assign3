list=[1,2,3,5.6,"hello",7.8]
string=[]
flt=[]
b=[]
for i in list:
    if type(i) == str:
        string.append(i)
    elif type(i)==float:
        flt.append(i)
    else:
        b.append(i)

print(string)
print(flt)
print(b)


''''

n=int(input(" enter row"))
for i in range(2,n+2):
    for j in range(1,i):
        print(j,end=" ")
    print('\r')


