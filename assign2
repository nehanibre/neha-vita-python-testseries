start=int(input())
stop=int(input())
for i in range(start,stop):
    if(i%7==0 and i%5==1):
        print(i,end=", ")

a=int(input())
dict={i:i**2  for i in range (1,a+1)}
print(dict)

a=str(input())
b=a.upper()
print(b)

a=int(input())
if(a%2==0):
    print("even")
else:
    print("odd")

age=int(input())
if age in range(8,12):
    print("you are allowed....welcome!!")
else:
    print("Sorry not allowed...bye!")
